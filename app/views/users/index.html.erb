

<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><img class="attachment user profile_image fallback" src=""></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", users_path(user) %></td>
      </tr>
      <% end %>
  </tbody>
</table>
